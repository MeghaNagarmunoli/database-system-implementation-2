{
    "files.associations": {
        "fstream": "cpp",
        "istream": "cpp",
        "ostream": "cpp",
        "iostream": "cpp",
        "random": "cpp",
        "new": "cpp",
        "*.tcc": "cpp",
        "optional": "cpp",
        "algorithm": "cpp",
        "typeinfo": "cpp",
        "streambuf": "cpp",
        "type_traits": "cpp",
        "string": "cpp",
        "iosfwd": "cpp",
        "string_view": "cpp"
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "googletest.failed",
                "settings": {
                    "foreground": "#f00"
                }
            },
            {
                "scope": "googletest.passed",
                "settings": {
                    "foreground": "#0f0"
                }
            },
            {
                "scope": "googletest.run",
                "settings": {
                    "foreground": "#0f0"
                }
            }
        ]
    },
    "gtest-adapter.debugConfig": [
        "(gdb) Launch"
    ]
}